# note: Table sys_user
class __User:
    user_id: int
    user_name: str
    user_full_name: str
    user_password: str
    user_status: UserStatus
    user_sex: UserSex
    user_avatar: str
    user_department_name: str
    user_type: UserType
    user_roles: List[str]
    user_email: str
    user_phone_number: str
    user_last_ip: str
    user_last_login_datetime: datetime.datetime
    user_create_by: int
    user_create_datetime: datetime.datetime
    user_remark: str